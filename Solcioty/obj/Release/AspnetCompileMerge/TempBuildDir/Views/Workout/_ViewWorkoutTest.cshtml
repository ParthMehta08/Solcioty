<div class="slider">
    <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="false">
        <ol class="carousel-indicatorsnone" style="display:none;">
            @if (Model.WorkoutInfo.Templates != null)
            {
                for (int i = 0; i < Model.WorkoutInfo.Templates.Count; i++)
                {
                    if (i == 0)
                    {
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    }
                    else
                    {
                        <li data-target="#myCarousel" data-slide-to="@i"></li>
                    }
                }
            }

        </ol>
        <!-- Indicators -->
        <!--<a class="left carousel-control" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
          <span class="sr-only">Previous</span>
        </a>-->
        <!--<a class="right carousel-control" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
          <span class="sr-only">Next</span>
        </a>-->
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            @if (Model.WorkoutInfo.Templates != null)
            {
                foreach (var template in Model.WorkoutInfo.Templates)
                {
                    if (template.NumberOfBasicVideos == 1 && template.NumberOfAlternateVideos == 1)
                    {
                        <div class="item">
                            <div class="Line" style="background:linear-gradient(to right, #@Model.ClientConfiguration.PrimaryGradientBackground, #e6e6e6)">
                                <div class="sidebar1" style="background-color:#@Model.ClientConfiguration.PrimaryBackground">
                                    <h3 style="color:#@Model.ClientConfiguration.PrimaryFontColor">P<br>R<br>I<br>M<br>A<br>R<br>Y</h3>
                                </div>
                                <div class="video1">
                                    <video width="99.6%" height="90%" style="padding: 0 0.2%;" autoplay loop muted="">
                                        <source src="@Url.Content($"AllVideos/{template.BasicVideos[0].VideoId}/{template.BasicVideos[0].VideoFile}")" type="video/mp4">
                                    </video>
                                    <div class="reps" style="color:#@Model.ClientConfiguration.PrimaryFontColor;background-color:#@Model.ClientConfiguration.PrimaryBackground">@template.BasicVideos[0].RepeatCount REPS</div>
                                </div>

                            </div>
                        </div>
                    }
                    if (template.NumberOfBasicVideos == 2 && template.NumberOfAlternateVideos == 2)
                    {
                        <div class="item">
                            <div class="Line1" style="background:linear-gradient(to right, #@Model.ClientConfiguration.PrimaryGradientBackground, #e6e6e6)">
                                <div class="sidebar1" style="background-color:#@Model.ClientConfiguration.PrimaryBackground">
                                    <h3>P<br>R<br>I<br>M<br>A<br>R<br>Y</h3>
                                </div>
                                @for (int i = 0; i < template.NumberOfBasicVideos; i++)
                                {
                                    <div class="video2">
                                        <video width="99.6%" height="90%" style="padding: 0 0.2%;" autoplay loop muted="">
                                            <source src="@Url.Content($"AllVideos/{template.BasicVideos[i].VideoId}/{template.BasicVideos[i].VideoFile}")" type="video/mp4">
                                        </video>
                                        <div class="reps" style="color:#@Model.ClientConfiguration.PrimaryFontColor;background-color:#@Model.ClientConfiguration.PrimaryBackground">@template.BasicVideos[i].RepeatCount REPS</div>
                                    </div>
                                }

                            </div>
                            <div class="Line1" style="background:linear-gradient(to right, #@Model.ClientConfiguration.AlternateGradientBackground, #e6e6e6)">
                                <div class="sidebar2" style="background-color:#@Model.ClientConfiguration.AlternateBackground">
                                    <h3>A<br>L<br>T<br>E<br>R<br>N<br>A<br>T<br>E</h3>
                                </div>
                                @for (int i = 0; i < template.NumberOfAlternateVideos; i++)
