                            <td style="width:15px;border:solid #e6e6e6;border-width: 1px 1px 0px 0px; "></td>
                            <td style="border:1px solid #e6e6e6;width:92%;text-align:center;">@template.BasicVideos[0].VideoTitle</td>
                        </tr>
                        <tr>
                            <td colspan="3" style="border: solid #e6e6e6;border-width: 0px 0px 1px 1px;"></td>
                            <td style="border:1px solid #e6e6e6;padding-top:10px; padding-bottom:10px; height:45px; text-align:center;">@template.BasicVideos[0].SmallDescription</td>

                        </tr>
                    </table>
                    <div style="min-height:650px;">
                        <h5>Description: </h5>
                        @{
                            HtmlString msg = new HtmlString(template.Description);
                        }
                        <span>@msg</span>
