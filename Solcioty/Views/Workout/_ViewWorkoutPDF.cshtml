@using Models

@model ModelHomePage
<form name="workout" action="@Url.Action("PrintWorkout","Workout")" method="post">
    <button type="submit" id="btnSubmit" class="btn btn-purple waves-effect waves-light pull-right">PRINT</button>
    @Html.Hidden("WorkoutHtml")
    <div class="workoutdetail">
        <h3>Workout: @Model.WorkoutInfo.WorkoutName</h3>
        @if (Model.WorkoutInfo.Templates != null)
